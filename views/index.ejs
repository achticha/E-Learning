            padding: 0;
            box-sizing: border-box;
            font-family: "Lexend", sans-serif;
        }

        a {
            text-decoration: none;
            color: #000;
        }

        img {
            object-fit: cover;
        }

        body {
            background-image: linear-gradient(135deg, rgb(99, 177, 255) 20%, rgb(219, 255, 244) 83%);
            background-attachment: fixed;
            background-size: cover;
            width: 100%;
            height: 100%;
        }

        nav {
            padding: 0 80px;
            display: flex;
            align-items: center;
            justify-content: space-between;
            height: 80px;
        }

        nav a.logo {
            font-size: 33px;
            color: #ffffff;
            font-weight: 600;
        }

        nav .login a {
            font-size: 20px;
        }

        nav .login button {
            padding: 8px 14px;
            border: none;
            cursor: pointer;
            background-color: transparent;
        }

        nav .login button.signup {
            background-color: #000;
            color: #fff;
            border-radius: 4px;
            margin-right: 14px;
        }

        header {
            padding: 0 80px;
            height: calc(100vh - 80px);
            display: flex;
            align-items: center;
            justify-content: space-between;
        }

        header .left {
