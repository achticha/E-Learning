            font-size: 80px;
            color: #ffffff;
        }

        header .left p {
            font-size: 18.5px;
            margin: 40px 0;
            color: #777;
        }

        .tooltip-container {
            margin: 70px 40px;
            --background: linear-gradient(135deg, rgb(0, 255, 214) 0%, rgb(8, 226, 96) 100%);
            position: relative;
            cursor: pointer;
            transition: all 0.4s cubic-bezier(0.23, 1, 0.32, 1);
            font-size: 18px;
            font-weight: 600;
            color: var(--color);
            padding: 0.7em 1.8em;
            border-radius: 8px;
            text-transform: uppercase;
            height: 60px;
            width: 180px;
            display: grid;
            place-items: center;
            border: 2px solid var(--color);
        }

        .text {
            background: #2e2c2c;
            border-radius: 8px;
            font-size: 24px;
            color: #fff;
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            display: grid;
            place-items: center;
            transform-origin: -100%;
            transform: scale(1);
            transition: all 0.4s cubic-bezier(0.23, 1, 0.32, 1);
        }

        .tooltip-container span:last-child {
            color: #fff;
            background: linear-gradient(135deg, rgb(0, 255, 214) 0%, rgb(8, 226, 96) 100%);
            font-size: 24px;
            position: absolute;
            top: 0%;
            left: 100%;
            width: 100%;
            height: 100%;
            border-radius: 8px;
            opacity: 1;
            /* background-color: var(--background); */
            z-index: -1;
            border: 2px solid var(--background);
            transform: scale(0);
            transform-origin: 0;
            transition: all 0.4s cubic-bezier(0.23, 1, 0.32, 1);
            display: grid;
            place-items: center;
        }

        .tooltip {
            color: #fff;
            position: absolute;
            top: 0;
            left: 50%;
            transform: translateX(-50%);
            padding: 0.5em 0.6em;
            opacity: 0;
            pointer-events: none;
            transition: all 0.4s cubic-bezier(0.23, 1, 0.32, 1);
            background: linear-gradient(135deg, rgb(255, 13, 0) 0%, rgb(226, 8, 142) 100%);
            z-index: -1;
            border-radius: 8px;
            scale: 0;
            transform-origin: 0 0;
            text-transform: capitalize;
            font-weight: 400;
            font-size: 16px;
            box-shadow: rgba(0, 0, 0, 0.25) 0 8px 15px;
        }

        .tooltip::before {
            position: absolute;
            content: "";
            height: 0.6em;
            width: 0.6em;
            bottom: -0.2em;
            left: 50%;
            transform: translate(-50%) rotate(45deg);
            background: var(--background);
        }

        .tooltip-container:hover .tooltip {
            top: -100%;
            opacity: 1;
            visibility: visible;
            pointer-events: auto;
            scale: 1;
            animation: shake 0.5s ease-in-out both;
        }

        .tooltip-container:hover {
            box-shadow: rgba(0, 0, 0, 0.25) 0 8px 15px;
            color: white;
            border-color: transparent;
        }

        .tooltip-container:hover span:last-child {
            transform: scale(1);
            left: 0;
        }

        .tooltip-container:hover .text {
            opacity: 0;
            top: 0%;
            left: 100%;
            transform: scale(0);
        }

        @keyframes shake {
            0% {
                rotate: 0;
            }

            25% {
                rotate: 7deg;
            }

            50% {
                rotate: -7deg;
            }

            75% {
                rotate: 1deg;
            }

            100% {
                rotate: 0;
            }
        }


        h2.separator {
            margin-top: 10vh;
            padding: 0 100px;
            font-size: 40px;
            color: #ffffff;

        }

        :root {
            --first-color: hsl(141, 32%, 51%);
            --first-color-light: hsl(206, 100%, 56%);
            --first-color-alt: hsl(32, 85%, 69%);
            --second-color: hsl(200, 100%, 50%);
            --dark-color: hsl(0, 0%, 100%);
            --white-color: hsl(212, 4%, 95%);
            --body-color: hsl(0, 0%, 100%);
            --container-color: hsl(0, 0%, 100%);
        }

        img {
            display: block;
            max-width: 100%;
            height: auto;
        }

        .container {
            display: flex;
            justify-content: space-between;
            margin: 0 0 50px;
            gap: 30px;
        }

        .containerview {
            display: flex;
            justify-content: space-between;
            margin: 0 0 50px;
            gap: 30px;
        }

        .card__container {
            padding: 1rem;

        }

        .card__content {
            padding: 1.5rem;
            margin-inline: 1.75rem;
            border-radius: 1.25rem;
            overflow: hidden;
        }

        .card__article {
            display: block;
            margin-top: 1.29vh;
            width: 300px;
            border-radius: 1.25rem;
        }

        .card__image {
            position: relative;
            background-color: var(--first-color-light);
            padding-top: 1.5rem;
            margin-bottom: -.75rem;
        }

        .card__article:hover img {
            transform: translateY(-40px) scale(1.3);
            z-index: 999;
            transition: transform 0.3s ease;
        }

        .card__data {
            background-color: var(--container-color);
            padding: 1.5rem 2rem;
            border-radius: 1rem;
            text-align: center;
            position: relative;
            z-index: 10;
        }


        .card__data h3,
        .card__data p {
            word-break: break-word;
            overflow-wrap: break-word;
        }

        .card__img {
            width: 180px;
            margin: 0 auto;
            position: relative;
            z-index: 5;
        }

        .card__shadow {
            width: 200px;
            height: 200px;
            background-color: var(--first-color-alt);
            border-radius: 50%;
            position: absolute;
            top: 3.75rem;
            left: 0;
            right: 0;
            margin-inline: auto;
            filter: blur(45px);
        }

        .card__name {
            margin-top: 10px;
            font-size: 20px;
            color: var(--second-color);
            margin-bottom: .75rem;
            word-break: break-word;
            overflow-wrap: break-word;
        }

        .card__description {
            font-size: 17.5px;
            display: flex;
            height: 100px;
            word-break: break-word;
            overflow-wrap: break-word;
        }

        .swiper-pagination-bullet {
            background-color: hsl(182, 31%, 59%);
            height: 20px;
            width: 20px;
            opacity: 1;
        }

        .swiper-pagination-bullet-active {
            background-color: var(--second-color);
        }

        @media screen and (max-width: 320px) {
            .card__data {
                padding: 1rem;
            }
        }

        @media screen and (min-width: 768px) {
            .card__content {
                margin-inline: 1rem;
            }

            .swiper-button-next,
            .swiper-button-prev {
                display: block;
            }
        }

        @media screen and (min-width: 1300px) {
