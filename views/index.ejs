                max-width: 1250px;
            }

        }


        .card {
            margin-bottom: 3rem;
            padding: 5rem;
            margin-top: -1rem;
        }


        .nft-shop .nft-listfri {
            display: grid;
            grid-template-columns: repeat(3, 1fr);
            /* grid-template-columns: repeat(auto-fit, minmax(25rem, 1fr)); */
            gap: 1.5rem;
            justify-content: center;
            align-items: flex-start;


        }

        .nft-shop .nft-listfri .nft-lists .item {
            background-color: white;
            padding: 1.8rem;
            border-radius: 2rem;
            box-shadow: var(--box-shadow);
            display: flex;
            flex-wrap: wrap;
            gap: 1.4rem;
            cursor: pointer;
            transition: all 0.3s ease;
            align-items: stretch;
            position: relative;
            padding: 1.8rem;
            border: 2px solid #ffffff;
            transition: 0.1s ease-out;
            overflow: visible;

        }

        .nft-shop .nft-listfri .nft-lists .item .info button .card-button {
            transform: translate(-50%, 125%);
            width: 40%;
            height: 3rem;
            border-radius: 1rem;
            border: none;
            background: linear-gradient(-45deg, #eb36ff 0%, #4dcdff 100% );
            box-shadow: 0 0 30px 5px rgba(255, 255, 255, 0.815);
            color: #fff;
            font-size: 1.3rem;
            text-align: center;
            padding: .6rem 1rem;
            position: absolute;
            left: 50%;
            bottom: 0;
            opacity: 0;
            transition: 0.3s ease-out;

        }

        .nft-shop .nft-listfri .nft-lists .item:hover {
            border: 4px solid #b5e6ff;
            box-shadow: 0 4px 18px 0 rgba(0, 0, 0, 0.25);
        }

        .nft-shop .nft-listfri .nft-lists .item:hover .info button .card-button {
            transform: translate(-50%, 50%);
            opacity: 1;
        }


        .nft-shop .nft-listfri .nft-lists .item img {
            width: 100%;
            height: 20rem;
            object-fit: cover;
            border-radius: 0.5rem;

        }

        .nft-shop .nft-listfri .nft-lists .item h5,
        .nft-shop .nft-listfri .nft-lists .item h6,
        .nft-shop .nft-listfri .nft-lists .item p {
            margin-top: 2vh;
            word-break: break-word;
            overflow-wrap: break-word;
        }

        .nft-shop .nft-listfri .nft-lists .item h5 {
            font-size: 20px;
        }

        .nft-shop .nft-listfri .nft-lists .item h6 {
            font-size: 16px;
        }

        .view-more {

            margin: -80px 0 40px;
            display: flex;
            justify-content: center;
        }

